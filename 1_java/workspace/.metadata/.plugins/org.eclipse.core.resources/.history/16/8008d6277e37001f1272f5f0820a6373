package zoo;

public class Ground {
	public static void main(String[] args) {
		//서로 다른 종류의 동물 세마리 울게하기 //사자, 고양이, 강아지
		
		Animal animal = new Animal("고양이");
		Cat cat = new Cat("고양이");
		Lion lion = new Lion("사자");
		Dog dog = new Dog("강아지");
		
		animal.crying();
		cat.crying();
		lion.crying();
		dog.crying();
		
		
	}
}
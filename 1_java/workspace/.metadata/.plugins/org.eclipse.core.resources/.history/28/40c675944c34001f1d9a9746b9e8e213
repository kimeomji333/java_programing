package day04;

import java.util.Scanner;

//5 메소드
public class MethodTest {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		sc.next();
		
		MethodTest m = new MethodTest();
		m.f(3);
		
		System.out.println(m);
	}
	
	//f(x) = 2x + 1
	int f(int x) {
		return 2*x + 1;
	}

}
